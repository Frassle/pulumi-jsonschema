{
  "name": "bootstraprc",
  "version": "1.0.0",
  "description": "A pulumi package generated from a json schema",
  "keywords": [
    "pulumi",
    "jsonschema"
  ],
  "homepage": "https://github.com/Frassle/pulumi-jsonschema",
  "repository": "https://github.com/Frassle/pulumi-jsonschema",
  "license": "Apache-2.0",
  "pluginDownloadURL": "github://api.github.com/Frassle",
  "parameterization": {
    "baseProvider": {
      "version": "0.1.1",
      "name": "jsonschema"
    },
    "parameter": ""
  },
  "functions": {
    "bootstraprc:index:read": {
      "description": "Read the given JSON into the object model",
      "inputs": {
        "required": [
          "json"
        ],
        "properties": {
          "json": {
            "type": "string"
          }
        }
      },
      "outputs": {
        "required": [
          "value"
        ],
        "properties": {
          "value": {
            "$ref": "#/types/bootstraprc:index:coreProperties"
          }
        }
      }
    },
    "bootstraprc:index:write": {
      "description": "Read the given JSON into the object model",
      "inputs": {
        "required": [
          "value"
        ],
        "properties": {
          "value": {
            "$ref": "#/types/bootstraprc:index:coreProperties"
          }
        }
      },
      "outputs": {
        "required": [
          "json"
        ],
        "properties": {
          "json": {
            "type": "string"
          }
        }
      }
    }
  },
  "types": {
    "bootstraprc:index:bootstrapVersion": {
      "description": "The major version of Bootstrap being used",
      "type": "integer",
      "enum": [
        {
          "value": 3
        },
        {
          "value": 4
        }
      ]
    },
    "bootstraprc:index:coreProperties": {
      "type": "object",
      "properties": {
        "additionalProperties": {
          "type": "object",
          "additionalProperties": {
            "$ref": "pulumi.json#/Any"
          }
        },
        "appStyles": {
          "type": "string",
          "description": "Import your custom styles here. Usually this endpoint file contains a list of @imports of your application styles."
        },
        "bootstrapCustomizations": {
          "type": "string",
          "description": "The .scss file path to be loaded after Bootstrap\u0027s _variables.scss file"
        },
        "bootstrapVersion": {
          "$ref": "#/types/bootstraprc:index:bootstrapVersion",
          "description": "The major version of Bootstrap being used"
        },
        "loglevel": {
          "$ref": "#/types/bootstraprc:index:loglevel",
          "description": "The verbosity of logging. Exclude this property to disable."
        },
        "preBootstrapCustomizations": {
          "type": "string",
          "description": "The .scss file path to be loaded before Bootstrap\u0027s _variables.scss file"
        },
        "scripts": {
          "$ref": "pulumi.json#/Any",
          "description": "unhandled schema: Json.Schema.DescriptionKeyword, Json.Schema.TypeKeyword"
        },
        "styleLoaders": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "An array of Webpack loader names. Order matters, and sass-loader is required."
        },
        "styles": {
          "$ref": "pulumi.json#/Any",
          "description": "unhandled schema: Json.Schema.DescriptionKeyword, Json.Schema.TypeKeyword"
        },
        "useCustomIconFontPath": {
          "type": "boolean",
          "description": "Set to true if using a custom icon font and you need to specify its path in your Sass files"
        },
        "useFlexbox": {
          "type": "boolean",
          "description": "Enables/disables the flexbox model available in Bootstrap 4"
        }
      },
      "required": [
        "bootstrapVersion",
        "styleLoaders"
      ]
    },
    "bootstraprc:index:loglevel": {
      "description": "The verbosity of logging. Exclude this property to disable.",
      "type": "string",
      "enum": [
        {
          "value": "debug"
        }
      ]
    }
  }
}